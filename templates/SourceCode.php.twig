<?php
{% if prototype.namespace|length %}

namespace {{ prototype.namespace }};
{% endif %}
{% if prototype.useStatements|length %}

{% for useStatement in prototype.useStatements %}
{% if useStatement.hasAlias %}
use {{ useStatement.className }} as {{ useStatement.alias }};
{% else %}
use {{ useStatement.className }};
{% endif %}
{% endfor %}
{% endif %}
{% for interface in prototype.interfaces %}

{{ generator.render(interface, variant)|raw }}
{% endfor %}
{% for class in prototype.classes %}

{{ generator.render(class, variant)|raw }}
{% endfor %}
